<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="com.zhixin.rd.memory.web.dao.spnew.ISpNewMapper">

    <select id="queryAllSp" resultType="java.util.HashMap">
		select * from sp_new
    </select>

    <select id="queryAllSpById" resultType="java.util.HashMap">
		select * from sp_new where id = #{id}
    </select>
</mapper>


